{
  "expo": {
    "name": "cred",
    "slug": "cred",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cred",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.manishmellow.cred"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e1fafa8d-2fb5-43e7-b67d-f110b7436010"
      }
    }
  }
}
